<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.example.demo.teams.mapper.TeamByYearMapper">
	<select id="findAllTeamsStats" resultType="com.example.demo.teams.model.TeamByYear">
		SELECT * 
		FROM teambyyear
		WHERE year = '2022-23'
	</select>
	
	<select id="findAllTeamsStatsFiveLastYears" resultType="com.example.demo.teams.model.TeamByYear">
		SELECT * 
		FROM teambyyear
	</select>
</mapper>