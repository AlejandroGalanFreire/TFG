<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.example.demo.scoreboard.mapper.ElemScoreboardMapper">
	<select id="getClasification" resultType="com.example.demo.scoreboard.model.ElemScoreboard">
		SELECT * 
		FROM scoreboard 
		WHERE league = #{league}
	</select>
</mapper>